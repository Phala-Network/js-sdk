{
  "name": "@phala/sdk",
  "version": "0.3.4-beta.0",
  "description": "Phala JS SDK",
  "homepage": "https://github.com/Phala-Network/js-sdk/tree/main/packages/sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Phala-Network/js-sdk.git",
    "directory": "packages/sdk"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "build:proto": "scripts/build_proto.sh",
    "dev": "tsup --watch",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@phala/typedefs": "^0.2.31",
    "@polkadot/api": "^9.4.1",
    "@polkadot/keyring": "^10.1.8",
    "@polkadot/util": "^10.1.8",
    "@polkadot/util-crypto": "^10.1.8",
    "@polkadot/wasm-crypto": "6.3.1",
    "axios": "^0.27.2",
    "crypto-browserify": "^3.12.0",
    "protobufjs": "^6.11.3",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@types/node": "^16.11.59",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16"
  }
}
